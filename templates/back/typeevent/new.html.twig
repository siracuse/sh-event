{% extends 'baseAdmin.html.twig' %}


{% block body %}



    <div class="card">
        <div class="card-header"><h1>Ajout d'un type d'événement</h1></div>
        <div class="card-body">
            {{ form_start(form) }}
                {{ form_row(form.name, {'attr': {'class': 'form-control'}}) }}
                {{ form_row(form.save, {'attr': {'class': 'btn btn-primary'}}) }}
            {{ form_end(form) }}
        </div>
    </div>

{% endblock %}